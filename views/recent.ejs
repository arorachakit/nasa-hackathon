<% include partials/header %> 
<div class="container ">
    <h1 class="display-3 pt-3">Recent News</h1>
        <% news.forEach(function(news){ %>
            <div class="my-4 pt-3 my-md-4 pt-md-4">
                    <h1 class="display-5"> <%= news.name %> </h1>
                    <div class="row">
                        <div class="col-md-8">
                            <p class="lead">
                                <%=news.description %>
                            </p>
                        </div>
                        <div class="col-md-4">
                            <img class="img-fluid" src="<%= news.image %>">
                        </div>
                    </div>
            </div>
        <%  }) %>
</div>
 

    
 <% include partials/footer %>